<h1>Lista de estudiantes</h1>

<table>
  <th>
    <tr>
      Nombre
    </tr>
    <tr>
      Apellido
    </tr>
    <tr>
      Fecha Nacimiento
    </tr>
  </th>
  <% @students.each do |student| %>
    <tr>
      <td><%= student.name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.birth_date %>
    </tr>
  <% end %>
</table>

<%=  link_to 'Registrar nuevo estudiante', new_student_path %>
